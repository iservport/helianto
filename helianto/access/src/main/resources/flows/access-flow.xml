<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE webflow PUBLIC "-//SPRING//DTD WEBFLOW//EN"
	"http://www.springframework.org/dtd/spring-webflow.dtd">

<webflow id="accessFlow" start-state="findEntity">

    <action-state id="findEntity">
        <action bean="accessAction"/>
        <transition on="one" to="findService"/>
        <transition on="many" to="entList.view"/>
        <transition on="none" to="noEntity.view"/>
    </action-state>
	
	<view-state id="noEntity.view" view="noEntity">
        <transition on="end" to="endAccess"/>
	</view-state>
	
	<!--subflow-state id="createUser" flow="userFlow">
        <transition on="endUser" to="findEntity"/>
        <transition on="error" to="noEntity.view"/>
	</subflow-state-->
	
	<view-state id="entList.view" view="entList">
        <transition on="select" to="findService"/>
	</view-state>
	
    <action-state id="findService">
        <action bean="accessAction"/>
        <transition on="one" to="welcome.view"/>
        <!--transition on="many" to="servList.view"/>
        <transition on="none" to="noServ.view"/-->
    </action-state>
	
	<!--view-state id="noService.view">
        <action bean="accessAction"/>
        <transition on="create" to="createContract"/>
	</view-state-->
	
	<!--subflow-state id="createContract" flow="xxx">
		<attribute-mapper>
			
		</attribute-mapper>
        <transition on="success" to="findService"/>
        <transition on="error" to="noService.view"/>
	</subflow-state-->
	
	<!--view-state id="servList.view">
        <action bean="accessAction"/>
        <transition on="select" to="welcome.view"/>
        <transition on="back" to="entList.view"/>
	</view-state-->
	
	<view-state id="welcome.view" view="welcome">
        <transition on="continue" to="endAccess"/>
        <!--transition on="back" to="servList.view"/-->
	</view-state>
	
	<end-state id="endAccess"/>
	
</webflow>

